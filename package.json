{
  "name": "ember-freestyle",
  "version": "0.12.1",
  "description": "Create a living styleguide for your Ember app.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/chrislopresto/ember-freestyle.git",
  "license": "MIT",
  "author": "Chris LoPresto",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prerelease": "./script/prerelease",
    "release": "./script/release",
    "postrelease": "./script/postrelease",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "dependencies": {
    "ember-cli-babel": "^7.23.0",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-cli-sass": "^10.0.0",
    "ember-modifier": "^2.1.0",
    "ember-runtime-enumerable-includes-polyfill": "^2.0.0",
    "glob": "^7.1.3",
    "sass": "^1.17.0",
    "strip-indent": "^3.0.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/render-modifiers": "^1.0.2",
    "@ember/test-helpers": "^2.1.4",
    "@glimmer/component": "^1.0.3",
    "@glimmer/tracking": "^1.0.3",
    "all-contributors-cli": "^6.14.1",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.10.1",
    "ember-cli": "~3.24.0",
    "ember-cli-autoprefixer": "^0.8.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-page-object": "^1.17.4",
    "ember-cli-qunit": "^4.4.0",
    "ember-cli-sass-lint": "^1.0.3",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-showdown": "^4.1.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-terser": "^4.0.1",
    "ember-code-snippet": "^3.0.0",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-native-dom-helpers": "^0.6.3",
    "ember-page-title": "^6.0.3",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.24.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.15.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.17.0",
    "eslint-config-ember": "0.3.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-ember": "^10.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "qunit-dom": "^1.6.0",
    "remarkable": "^2.0.0",
    "standard-version": "^7.0.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://chrislopresto.github.io/ember-freestyle/",
    "versionCompatibility": {
      "ember": ">= 2.4.0"
    }
  }
}
